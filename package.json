  "bugs": {
    "url": "https://github.com/romiaujla/tru-king-server/issues"
  },
  "homepage": "https://github.com/romiaujla/tru-king-server#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.18.2",
    "helmet": "^3.21.0",
    "knex": "^0.19.4",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^6.2.0",
